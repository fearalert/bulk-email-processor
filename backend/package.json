{
  "name": "bulk-email-processor",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec \"ts-node src/index.ts\"",
    "start": "ts-node src/index.ts",
