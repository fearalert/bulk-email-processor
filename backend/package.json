  "dependencies": {
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "p-limit": "^3.1.0",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.5"
